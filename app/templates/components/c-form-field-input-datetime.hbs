{{c-form-field-input-date
    label=label
    model=model
    field=field
    description=description
    required=required
    disabled=disabled
    class=class
    time=time
    action=(action "setTime")
    inputless=inputless
    placeholder=placeholder
}}

{{input
  type="text"
  value=time
  class="form-field-time-picker"
  key-press="setTime"
  placeholder=placeholder
}}
