{{#if label}}
  {{c-form-field-label fieldComponent=this}}
{{/if}}

{{c-form-star-rating value=(mut (get model field)) editable=true onSelect=onSelect}}

{{#if description}}
  <div class="description">{{description}}</div>
{{/if}}

{{#if error}}
  <div class="error-message">
      {{error}}
  </div>
{{/if}}
